{
  "name": "ecoleta-api",
  "version": "0.0.1",
  "description": "api da aplicação ecoleta",
  "main": "server.js",
  "scripts": {
    "start": "npm run knex:init && npm run dev",
    "dev": "ts-node-dev src/server/server.ts",
    "knex:migrate": "knex --knexfile knexfile.ts migrate:latest",
    "knex:seed": "knex --knexfile knexfile.ts seed:run",
    "knex:init": "npm run knex:migrate && npm run knex:seed"
  },
  "keywords": [
    "rocketseat",
    "next",
    "level",
    "week",
    "frontend",
    "backend",
    "mobile",
    "react",
    "node",
    "expo",
    "react-native"
  ],
  "author": "matheus do e santos",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "knex": "^0.21.1",
    "sqlite3": "^4.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/cors": "^2.8.6",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.9.3"
  }
}
